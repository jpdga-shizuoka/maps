sed -E "s/>00<\/tspan/>12<\/tspan/" tee00.svg > fronttee12.svg
sed -E "s/>00<\/tspan/>13<\/tspan/" tee00.svg > fronttee13.svg
sed -E "s/>00<\/tspan/>14<\/tspan/" tee00.svg > fronttee14.svg
sed -E "s/>00<\/tspan/>15<\/tspan/" tee00.svg > fronttee15.svg
