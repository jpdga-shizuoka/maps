sed -E "s/>00<\/tspan/>18<\/tspan/" tee00.svg > backtee18.svg
sed -E "s/>00<\/tspan/>19<\/tspan/" tee00.svg > backtee19.svg

sed -E "s/>0<\/tspan/>1<\/tspan/" tee0.svg > fronttee1.svg
